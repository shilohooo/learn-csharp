<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  Add Resources Here  -->
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Dark">
            <!--  侧边栏背景色  -->
            <SolidColorBrush Color="#222226" x:Key="SidebarBgColor" />
            <!--  侧边栏边框颜色  -->
            <SolidColorBrush Color="#343434" x:Key="SidebarBorderColor" />
            <!--  内容区域背景色  -->
            <SolidColorBrush Color="#111113" x:Key="ContentBgColor" />
            <!--  当前菜单项的背景色  -->
            <SolidColorBrush Color="#38383b" x:Key="SelectedMenuItemBgColor" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>